    "canvas": "^2.11.2",
    "color": "^4.2.3",
    "copyfiles": "^2.4.1",
    "discord.js": "^14.12.1",
    "dotenv": "^16.4.5",
    "ffmpeg": "0.0.4",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "font-color-contrast": "^11.1.0",
    "fuzzy-search": "^3.2.1",
    "libsodium-wrappers": "^0.7.13",
    "minecraft-server-util": "^5.4.4",
    "mongoose": "^8.2.3",
    "nanoid": "^5.0.6",
    "opusscript": "0.1.1",
    "path": "^0.12.7",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.2",
    "tsconfig-paths": "^4.2.0",
