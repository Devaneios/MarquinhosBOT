    "discord-player-deezer": "^2.1.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "font-color-contrast": "^11.1.0",
    "sharp": "^0.33.3",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/color": "^3.0.6",
    "@types/fuzzy-search": "^2.1.5",
    "@types/node": "^20.12.5",
